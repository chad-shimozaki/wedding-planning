          <div class="card">
            <img src="<%= Event.where({ :chronological_order => 1 }).first.photo_url %>" class="card-img-top" alt="Image 1">
            <div class="card-body">
              <h5 class="card-title"><%= Event.where({ :chronological_order => 1 }).first.name %></h5>
              <ul class="no-bullets">
                <li>
                  <p class="card-text"><strong>Date</strong>: <%= Event.where({ :id => Event.where({ :chronological_order => 1 }).first.id }).first.date.strftime("%B %d, %Y") %></p>
                </li>
                <li>
                  <p class="card-text"><strong>Venue</strong>: <%= Venue.where({ :id => Event.where({ :chronological_order => 1 }).first.venue_id }).first.name %></p>
                </li>
                <li>
